         <ValidMax>70000</ValidMax>
         <FillValue>-99999.999</FillValue>
         <Field>
            <Qualifier>Magnitude</Qualifier>
            <FieldQuantity>Magnetic</FieldQuantity>
         </Field>
      </Parameter>
