            <Units>km</Units>
            <UnitsConversion>1e3&gt;m</UnitsConversion>
            <CoordinateSystem>
                <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
                <CoordinateSystemName>SE</CoordinateSystemName>
            </CoordinateSystem>
            <Support>
                <Qualifier>Component.I</Qualifier>
                <SupportQuantity>Positional</SupportQuantity>
            </Support>
        </Parameter>
        <Parameter>
            <Name>Yse</Name>
            <ParameterKey>Column 3</ParameterKey>
            <Description>Y component of spacecraft position in SE coordinates</Description>
            <Units>km</Units>
            <UnitsConversion>1e3&gt;m</UnitsConversion>
            <CoordinateSystem>
                <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
                <CoordinateSystemName>SE</CoordinateSystemName>
            </CoordinateSystem>
            <Support>
                <Qualifier>Component.J</Qualifier>
                <SupportQuantity>Positional</SupportQuantity>
            </Support>
