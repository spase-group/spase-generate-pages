          {
            "PersonID": "spase://SMWG/Person/Ronald.P.Lepping",
            "Role": "PrincipalInvestigator"
          },
          {
            "PersonID": "spase://SMWG/Person/Adam.Szabo",
            "Role": "DataProducer"
          }
        ],
        "InformationURL": {
