<Spase xmlns="http://www.spase-group.org/data/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.spase-group.org/data/schema  http://www.spase-group.org/data/schema/spase-2_2_0.xsd">
    <Version>2.2.2</Version>
    <NumericalData>
        <ResourceID>spase://VHO/NumericalData/Wind/MFI/PT15S</ResourceID>
        <ResourceHeader>
            <ResourceName>Wind MFI 15-sec magnetic field data, solar wind only</ResourceName>
            <ReleaseDate>2012-03-09T00:00:00</ReleaseDate>
            <Description>Data consist of 15-sec averages of magnetic field 
magnitude and GSE Cartesian components, from the MFI magnetometer on 
Wind.  Data are for only the solar wind phases of the Wind orbit.</Description>
            <Acknowledgement>Dr. Adam Szabo</Acknowledgement>
            <Contact>
                <PersonID>spase://SMWG/Person/Adam.Szabo</PersonID>
                <Role>PrincipalInvestigator</Role>
            </Contact>
            <Contact>
                <PersonID>spase://SMWG/Person/Natalia.E.Papitashvili</PersonID>
                <Role>DataProducer</Role>
            </Contact>
            <InformationURL>
                <Name>Readme file at SPDF</Name>
                <URL>http://spdf.gsfc.nasa.gov/pub/data/wind/mfi/ascii/15sec_ascii/00readme</URL>
                <Description>Details on creation and content of this data set</Description>
            </InformationURL>
        </ResourceHeader>
        <AccessInformation>
            <RepositoryID>spase://SMWG/Repository/NASA/GSFC/SPDF</RepositoryID>
            <Availability>Online</Availability>
            <AccessRights>Open</AccessRights>
            <AccessURL>
                <Name>SPDF FTP area</Name>
                <URL>ftp://spdf.gsfc.nasa.gov/pub/data/wind/mfi/ascii/15sec_ascii/</URL>
            </AccessURL>
            <AccessURL>
                <Name>SPDF HTTP area</Name>
                <URL>http://spdf.gsfc.nasa.gov/pub/data/wind/mfi/ascii/15sec_ascii/</URL>
            </AccessURL>
            <AccessURL>
                <Name>OMNIWeb-Plus</Name>
                <URL>http://ftpbrowser.gsfc.nasa.gov/wind_mag15s.html</URL>
                <Description>FTPBrowser graphical display interface to ASCII data</Description>
            </AccessURL>
            <Format>Text</Format>
            <Acknowledgement>Please acknowledge Principal Investigator A. Szabo, NASA/GSFC, 
and J. King and N. Papitashvili, NASA/GSFC/SPDF, data set producers.</Acknowledgement>
        </AccessInformation>
        <ProcessingLevel>Calibrated</ProcessingLevel>
        <InstrumentID>spase://SMWG/Instrument/Wind/MFI</InstrumentID>
        <MeasurementType>MagneticField</MeasurementType>
        <TemporalDescription>
            <TimeSpan>
                <StartDate>1994-11-16T00:00:00</StartDate>
                <RelativeStopDate>-P30D</RelativeStopDate>
            </TimeSpan>
            <Cadence>PT15S</Cadence>
        </TemporalDescription>
        <ObservedRegion>Heliosphere.NearEarth</ObservedRegion>
        <Parameter>
            <Name>Time</Name>
            <ParameterKey>Column_1, Column_2, Column_3, Column_4, Column_5</ParameterKey>
            <Description>Universal time at start of average</Description>
            <Cadence>PT15S</Cadence>
            <Structure>
                <Size>5</Size>
                <Description>Year, day-of-year, hour, minute, second at start of average</Description>
            </Structure>
            <Support>
                <SupportQuantity>Temporal</SupportQuantity>
            </Support>
        </Parameter>
        <Parameter>
            <Name>Magnetic Field Magnitude</Name>
            <ParameterKey>Column_10</ParameterKey>
            <Description>Average magnitude magnitude</Description>
            <Cadence>PT15S</Cadence>
            <Units>nT</Units>
            <Field>
                <Qualifier>Magnitude</Qualifier>
                <FieldQuantity>Magnetic</FieldQuantity>
            </Field>
        </Parameter>
        <Parameter>
            <Name>Magnetic Field Vector</Name>
            <ParameterKey>Column_7, Column_8, Column_9</ParameterKey>
            <Description>GSE Cartesian components of magnetic field vector</Description>
            <Cadence>PT15S</Cadence>
            <Units>nT</Units>
            <CoordinateSystem>
                <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
                <CoordinateSystemName>GSE</CoordinateSystemName>
            </CoordinateSystem>
            <Structure>
                <Size>3</Size>
                <Description>GSE Cartesian components in X,Y,Z sequence</Description>
            </Structure>
            <Field>
                <Qualifier>Vector</Qualifier>
                <FieldQuantity>Magnetic</FieldQuantity>
            </Field>
        </Parameter>
    </NumericalData>
</Spase>
